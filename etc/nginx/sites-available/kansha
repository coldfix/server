server {
    listen      443 ssl;
    listen [::]:443 ssl;
    server_name kansha.coldfix.de
                kansha.coldfix.eu
                kansha.fireflake.de
                kansha.fireflake.eu;
    location / {
        proxy_pass http://kansha:8080;
    }
    access_log /var/log/nginx/access_kansha.log;
}
