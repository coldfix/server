server {
    listen      443 ssl http2;
    listen [::]:443 ssl http2;
    server_name pics.coldfix.de pix.coldfix.de picbox.coldfix.de
                pics.coldfix.eu pix.coldfix.eu picbox.coldfix.eu
                pics.fireflake.de pix.fireflake.de picbox.fireflake.de
                pics.fireflake.eu pix.fireflake.eu picbox.fireflake.eu;
    location / {
        proxy_pass http://pycbox:5000;
    }
    access_log /var/log/nginx/access_pics.log;
    client_max_body_size 100m;
}
